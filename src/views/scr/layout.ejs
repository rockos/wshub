<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <!--
    <div class="navbar-header">
    -->
    <div class="navbar-inner">
        <button type="button" class="navbar-toggle" 
            data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/"><%= frameNavi.name %></a>
    </div>
    <div class="collapse navbar-collapse navbar-ex1-collapse">
        <ul class="nav navbar-nav">
            <% for (var i = 0, max = frameNavi.topMenu.length; i < max; i++) { %>
            <li class="dropdown">
            <% if (frameNavi.topMenu[i].subMenu.length) { %>
            <a href="<%= frameNavi.topMenu[i].href %>" 
                class="dropdown-toggle" data-toggle="dropdown">
                <%= frameNavi.topMenu[i].name %><b class="caret"></b></a>
            <ul class="dropdown-menu">
                <% for (var j = 0, 
                maxj = frameNavi.topMenu[i].subMenu.length; j < maxj; j++) { %>
                <li><a href="<%= frameNavi.topMenu[i].subMenu[j].href %>">
                    <%= frameNavi.topMenu[i].subMenu[j].name %></a></li>
                <% } %>
            </ul>
            <% } else { %>
            <a href="<%= frameNavi.topMenu[i].href %>" 
                class="dropdown-toggle" >
                <%= frameNavi.topMenu[i].name %></a>
            <% } %>
            </li>
            <% } %>
