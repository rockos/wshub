<% layout('layout') -%>
<div class="container">
    <form class="form-horizontal" role="form" method="POST" action="/scr/610">
        <dl class="dl-horizontal">
            <dt>
            <label class="control-label" for="InputEmail1">Email address</label>
            </dt>
            <dd>
            <input type="email"  class="input-lg col-mid-7" 
               name="userid" value="" 
               id="InputEmail1" placeholder="Enter email">
            </dd>
            <br>
            <dt>
            <label class="control-label" for="InputPassword1">パスワード</label>
            </dt>
            <dd>
            <input type="password" class="input-lg col-mid-7" 
               name="password" value="" 
               id="InputPassword1" placeholder="Password">
               </dd>
               <br>
            <dd>
            <button type="submit" class="btn btn-primary input-medium"
               name="610_signin" value='true'>サインイン</button>
            <button type="submit" class="btn btn-default input-medium"
               name="610_cancel" value='true'>キャンセル</button>
            </dd>
        </dl>
        
    </form>
</div>
